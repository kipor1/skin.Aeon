        <textcolor>a7ffffff</textcolor>
        <align>center</align>
        <font>Font_ProgressLabelSmall</font>
        <animation effect="slide" start="0,22" end="0,22" time="0" condition="IsEmpty(Control.GetLabel(3)) + IsEmpty(Control.GetLabel(4))">Conditional</animation>
