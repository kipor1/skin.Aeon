<window id="56">
    <defaultcontrol>6</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <controls>
        <control type="multiimage">
            <include>BackgroundVarsGlobal</include>
            <include>BackgroundVarsSettings</include>
            <info>Skin.String(CustomSettings)</info>
        </control>
        <control type="group">
            <include>FullScreenDimensions</include>
            <animation type="WindowOpen" reversible="false" condition="!Window.Previous(4)">
                <effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
                <effect type="fade" start="0" end="100" time="300" />
            </animation>
            <animation type="WindowClose" reversible="false" condition="!Window.Next(4)">
                <effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
                <effect type="fade" start="100" end="0" time="300" />
            </animation>
            <include>Furniture_SettingsBackPanel</include>
            <control type="label">
                <description>sectiontitle</description>
                <label>31160</label>
                <include>Objects_SettingsSectionTitle</include>
            </control>
            <control type="label">
                <description>sectiontitle</description>
                <posx>462</posx>
                <posy>105</posy>
                <width>1404</width>
                <height>74</height>
                <label>31231</label>
                <textcolor>FFbec0c2</textcolor>
                <include>Animation_CommonFadeOpenClose</include>
                <font>Font_SettingsSectionTitle</font>
            </control>
            <include>Settings_ProfileSettingsButtons</include>
            <control type="grouplist" id="9000">
                <include>Objects_SettingsControlAreaVars</include>
                <visible>Control.HasFocus(6) | Control.HasFocus(4)</visible>
                <onup>2</onup>
                <ondown>2</ondown>
                <onleft>6</onleft>
                <onright>6</onright>
                <control type="radiobutton" id="4">
                    <include>Objects_SettingsDefaultRadioButton</include>
                    <label>31162</label>
                </control>
            </control>
            <control type="list" id="2">
                <posx>387</posx>
                <posy>180</posy>
                <width>1404</width>
                <height>784</height>
                <onleft>7</onleft>
                <onright>7</onright>
                <animation effect="fade" start="0" end="100" time="125">WindowOpen</animation>
                <animation effect="fade" start="100" end="0" time="125">WindowClose</animation>
                <onup>4</onup>
                <ondown>4</ondown>
                <scrolltime>0</scrolltime>
                <viewtype label="$LOCALIZE[535] 2">list</viewtype>
                <pagecontrol>60</pagecontrol>
                <visible>Control.HasFocus(7) | Control.HasFocus(2)</visible>
                <itemlayout height="74">
                    <control type="image">
                        <posx>0</posx>
                        <posy>0</posy>
                        <width>1404</width>
                        <height>74</height>
                        <texture>settings/settings_radiobuttonoff.png</texture>
                    </control>
                    <control type="label">
                        <posx>75</posx>
                        <posy>0</posy>
                        <width>1404</width>
                        <height>74</height>
                        <font>Font_SettingsOptions</font>
                        <aligny>center</aligny>
                        <textcolor>FF939597</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label] ($INFO[ListItem.Label2])</label>
                        <visible>!IsEmpty(ListItem.Label2)</visible>
                    </control>
                    <control type="label">
                        <posx>75</posx>
                        <posy>0</posy>
                        <width>1404</width>
                        <height>74</height>
                        <font>Font_SettingsOptions</font>
                        <aligny>center</aligny>
                        <textcolor>FF939597</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label]</label>
                        <visible>IsEmpty(ListItem.Label2)</visible>
                    </control>
                </itemlayout>
                <focusedlayout height="74">
                    <control type="image">
                        <posx>0</posx>
                        <posy>0</posy>
                        <width>1404</width>
                        <height>74</height>
                        <texture>settings/settings_radiobuttonofffocus.png</texture>
                        <visible>!Control.HasFocus(4)</visible>
                    </control>
                    <control type="label">
                        <posx>75</posx>
                        <posy>0</posy>
                        <width>1404</width>
                        <height>74</height>
                        <font>Font_SettingsOptions</font>
                        <aligny>center</aligny>
                        <textcolor>FFFFFFFF</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label] ($INFO[ListItem.Label2])</label>
                        <visible>!IsEmpty(ListItem.Label2)</visible>
                    </control>
                    <control type="label">
                        <posx>75</posx>
                        <posy>0</posy>
                        <width>1404</width>
                        <height>74</height>
                        <font>Font_SettingsOptions</font>
                        <aligny>center</aligny>
                        <textcolor>FFFFFFFF</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label]</label>
                        <visible>IsEmpty(ListItem.Label2)</visible>
                    </control>
                </focusedlayout>
            </control>
            <include>Furniture_SettingsFrontPanel</include>
        </control>
        <!--<include>Background_ContextBlack</include>-->
        <include>Furniture_Debug</include>
    </controls>
</window>