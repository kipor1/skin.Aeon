<window id="122">
  <defaultcontrol>2</defaultcontrol>
  <coordinates>
    <system>1</system>
    <posx>360</posx>
    <posy>82</posy>
  </coordinates>
  <include>dialogeffect</include>
  <controls>
    <control type="image">
      <description>background image</description>
      <posx>0</posx>
      <posy>0</posy>
      <width>1200</width>
      <height>915</height>
      <colordiffuse>DDFFFFFF</colordiffuse>
      <texture border="40">DialogBack.png</texture>
    </control>
    <control type="image">
      <description>Heading Background</description>
      <posx>45</posx>
      <posy>52</posy>
      <width>1110</width>
      <height>60</height>
      <texture border="5">ThumbFO.png</texture>
    </control>
    <control type="label" id="3">
      <description>heading label</description>
      <posx>60</posx>
      <posy>52</posy>
      <width>1080</width>
      <height>60</height>
      <align>center</align>
      <aligny>center</aligny>
      <font>font18</font>
      <textcolor>black</textcolor>
    </control>
    <control type="label" id="4">
      <description>No Presets at all label</description>
      <posx>60</posx>
      <posy>300</posy>
      <width>1080</width>
      <height>45</height>
      <label>13389</label>
      <align>center</align>
      <font>font13</font>
      <textcolor>white</textcolor>
    </control>
    <control type="list" id="2">
      <posx>68</posx>
      <posy>128</posy>
      <width>1065</width>
      <height>660</height>
      <onleft>61</onleft>
      <onright>61</onright>
      <onup>2</onup>
      <ondown>2</ondown>
      <pagecontrol>61</pagecontrol>
      <scrolltime>200</scrolltime>
      <itemlayout height="60">
        <control type="image">
          <posx>0</posx>
          <posy>0</posy>
          <width>1065</width>
          <height>52</height>
          <texture border="5">button-nofocus.png</texture>
        </control>
        <control type="label">
          <posx>15</posx>
          <posy>0</posy>
          <width>1035</width>
          <height>52</height>
          <font>font12</font>
          <align>left</align>
          <aligny>center</aligny>
          <selectedcolor>blue</selectedcolor>
          <info>ListItem.Label</info>
        </control>
      </itemlayout>
      <focusedlayout height="60">
        <control type="image">
          <posx>0</posx>
          <posy>0</posy>
          <width>1065</width>
          <height>52</height>
          <visible>!Control.HasFocus(2)</visible>
          <texture border="5">button-nofocus.png</texture>
        </control>
        <control type="image">
          <posx>0</posx>
          <posy>0</posy>
          <width>1065</width>
          <height>52</height>
          <visible>Control.HasFocus(2)</visible>
          <texture border="5">button-focus.png</texture>
        </control>
        <control type="label">
          <posx>15</posx>
          <posy>0</posy>
          <width>1035</width>
          <height>52</height>
          <font>font12</font>
          <align>left</align>
          <aligny>center</aligny>
          <selectedcolor>blue</selectedcolor>
          <info>ListItem.Label</info>
        </control>
      </focusedlayout>
    </control>
    <control type="label">
      <description>number of files/pages in list text label</description>
      <posx>1125</posx>
      <posy>802</posy>
      <width>1050</width>
      <font>font12</font>
      <align>right</align>
      <scroll>true</scroll>
      <textcolor>white</textcolor>
      <label>($INFO[Container(2).NumItems]) $LOCALIZE[31025] - $LOCALIZE[31024] ($INFO[Container(2).CurrentPage]/$INFO[Container(2).NumPages])</label>
      <animation effect="slide" time="0" end="-112,0" condition="Control.IsVisible(61)">Conditional</animation>
    </control>
    <control type="spincontrol" id="61">
      <description>Next page button</description>
      <posx>1035</posx>
      <posy>807</posy>
      <subtype>page</subtype>
      <font>font12</font>
      <onleft>2</onleft>
      <onright>2</onright>
      <ondown>2</ondown>
      <onup>2</onup>
      <textcolor>-</textcolor>
      <showonepage>false</showonepage>
    </control>
  </controls>
</window>