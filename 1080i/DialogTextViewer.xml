<window id="147">
  <defaultcontrol>61</defaultcontrol>
  <coordinates>
    <system>1</system>
    <posx>0</posx>
    <posy>0</posy>
  </coordinates>
  <include>Animation_DialogPopup</include>
  <controls>
    <control type="image">
      <description>background image</description>
      <posx>0</posx>
      <posy>0</posy>
      <width>1920</width>
      <height>1080</height>
      <texture>wall/wall_vignette.png</texture>
    </control>
    <control type="image">
      <description>background image</description>
      <posx>0</posx>
      <posy>0</posy>
      <width>1920</width>
      <height>1080</height>
      <texture>dialogs/textviewer_back.png</texture>
    </control>
    <control type="button">
      <description>Close Window button</description>
      <posx>300</posx>
      <posy>0</posy>
      <width>96</width>
      <height>48</height>
      <label>-</label>
      <font>-</font>
      <onclick>PreviousMenu</onclick>
      <texturefocus>DialogCloseButton-focus.png</texturefocus>
      <texturenofocus>DialogCloseButton.png</texturenofocus>
      <onleft>61</onleft>
      <onright>61</onright>
      <onup>61</onup>
      <ondown>61</ondown>
      <visible>system.getbool(input.enablemouse)</visible>
    </control>
    <control type="group">
      <animation effect="fade" delay="400" start="0" end="100" time="200">WindowOpen</animation>
      <animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
      <control type="label" id="1">
        <description>header label</description>
        <posx>210</posx>
        <posy>90</posy>
        <width>1500</width>
        <include>Objects_SettingsSectionTitle</include>
        <label>-</label>
        <align>center</align>
      </control>
      <control type="textbox" id="5">
        <description>textarea</description>
        <posx>210</posx>
        <posy>225</posy>
        <width>1500</width>
        <height>765</height>
        <label>-</label>
        <align>justify</align>
        <pagecontrol>61</pagecontrol>
      </control>
      <control type="scrollbar" id="61">
        <posx>-24</posx>
        <posy>225</posy>
        <width>98</width>
        <height>765</height>
        <visible allowhiddenfocus="true">Control.HasFocus(61)</visible>
        <texturesliderbackground>windows/scrollbarbacklist.png</texturesliderbackground>
        <texturesliderbar>-</texturesliderbar>
        <texturesliderbarfocus>-</texturesliderbarfocus>
        <textureslidernib>windows/scrollbarnub-nofocus.png</textureslidernib>
        <textureslidernibfocus>windows/scrollbarnub-focus.png</textureslidernibfocus>
        <onleft>61</onleft>
        <onright>61</onright>
        <ondown>61</ondown>
        <onup>61</onup>
        <showonepage>true</showonepage>
        <orientation>vertical</orientation>
      </control>
    </control>
  </controls>
  <include>SideBladeRight</include>
  <include>Clock</include>
</window>