<window type="dialog" id="2600">
  <defaultcontrol>3</defaultcontrol>
  <zorder>1</zorder>
  <coordinates>
    <system>1</system>
    <posx>0</posx>
    <posy>0</posy>
  </coordinates>
  <controls>
    <!-- Background_Wallpaper -->
    <control type="multiimage">
      <include>FullscreenDimensions</include>
      <include>BackgroundTimings</include>
      <imagepath>special://skin/extras/backdrops/winter</imagepath>
      <info>Skin.String(CustomWeather)</info>
      <visible>!Skin.String(CustomWeatherFolder)</visible>
      <visible>System.Date(12-21,03-21)</visible>
    </control>
    <control type="multiimage">
      <include>FullscreenDimensions</include>
      <include>BackgroundTimings</include>
      <imagepath>special://skin/extras/backdrops/spring</imagepath>
      <info>Skin.String(CustomWeather)</info>
      <visible>!Skin.String(CustomWeatherFolder)</visible>
      <visible>System.Date(03-21,06-21)</visible>
    </control>
    <control type="multiimage">
      <include>FullscreenDimensions</include>
      <include>BackgroundTimings</include>
      <imagepath>special://skin/extras/backdrops/summer</imagepath>
      <info>Skin.String(CustomWeather)</info>
      <visible>!Skin.String(CustomWeatherFolder)</visible>
      <visible>System.Date(06-21,09-23)</visible>
    </control>
    <control type="multiimage">
      <include>FullscreenDimensions</include>
      <include>BackgroundTimings</include>
      <imagepath>special://skin/extras/backdrops/autumn</imagepath>
      <info>Skin.String(CustomWeather)</info>
      <visible>!Skin.String(CustomWeatherFolder)</visible>
      <visible>System.Date(09-23,12-21)</visible>
    </control>
    <control type="multiimage">
      <include>Background_ImageFolder</include>
      <info>Skin.String(CustomWeatherFolder)</info>
      <visible>Skin.String(CustomWeatherFolder)</visible>
      <visible>!Skin.HasSetting(CustomWeather)</visible>
    </control>
    <control type="multiimage">
      <include>Background_ImageFolder</include>
      <imagepath>weather/$INFO[Weather.FanartCode]</imagepath>
      <visible>Skin.String(CustomWeatherFolder)</visible>
      <visible>Skin.HasSetting(CustomWeather)</visible>
    </control>
    <!-- Background_Wallpaper end -->
    <control type="group">
      <posx>744</posx>
      <posy>424</posy>
      <width>434</width>
      <height>232</height>
      <animation type="WindowOpen" reversible="false">
        <effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
        <effect type="fade" start="0" end="100" time="300" />
      </animation>
      <animation type="WindowClose" reversible="false">
        <effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
        <effect type="fade" start="100" end="0" time="200" />
      </animation>
      <control type="image">
        <description>background top image</description>
        <posx>0</posx>
        <posy>-30</posy>
        <width>412</width>
        <height>30</height>
        <texture>dialogs/context_top.png</texture>
      </control>
      <control type="image">
        <description>background image</description>
        <posx>0</posx>
        <posy>0</posy>
        <width>412</width>
        <height>135</height>
        <texture>dialogs/context_middle.png</texture>
      </control>
      <control type="image">
        <description>background bottom image</description>
        <posx>0</posx>
        <posy>135</posy>
        <width>412</width>
        <height>27</height>
        <texture>dialogs/context_bottom.png</texture>
      </control>
      <control type="group">
        <posx>4</posx>
        <posy>0</posy>
        <width>410</width>
        <height>135</height>
        <include>Animation_CommonFadeOpenClose</include>
        <control type="label">
          <posx>0</posx>
          <posy>0</posy>
          <width>405</width>
          <height>60</height>
          <label>396</label>
          <textcolor>FF8f8f8f</textcolor>
          <aligny>center</aligny>
          <align>center</align>
          <font>Font_SettingsSectionTitle</font>
        </control>
        <control type="label">
          <posx>8</posx>
          <posy>45</posy>
          <width>390</width>
          <height>60</height>
          <info>Weather.Location</info>
          <textcolor>FF8f8f8f</textcolor>
          <aligny>center</aligny>
          <align>center</align>
          <font>Font_SettingsOptions</font>
        </control>
        <control type="spincontrolex" id="3">
          <description>change location button</description>
          <posx>202</posx>
          <posy>90</posy>
          <width>60</width>
          <textcolor>00bbbbbb</textcolor>
          <focusedcolor>00FFFFFF</focusedcolor>
          <textureup>settings/spin-up.png</textureup>
          <texturedown>settings/spin-down.png</texturedown>
          <textureupfocus>settings/spin-up-focus.png</textureupfocus>
          <texturedownfocus>settings/spin-down-focus.png</texturedownfocus>
          <font>Font_SettingsOptions</font>
          <disabledcolor>00d0d0d0</disabledcolor>
          <aligny>center</aligny>
          <align>-</align>
          <textoffsetx>195</textoffsetx>
          <spinposx>-2</spinposx>
          <spinwidth>60</spinwidth>
          <spinheight>98</spinheight>
          <include>Animations_SettingsButtonFade</include>
        </control>
      </control>
    </control>
  </controls>
</window>