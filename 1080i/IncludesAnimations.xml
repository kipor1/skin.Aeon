<includes>
  <include name="Animation_SubMenu">
    <animation type="Visible" reversible="false">
      <effect type="slide" start="0,-60" end="0,0" time="700" tween="cubic" easing="inout" />
      <effect type="fade" start="0" end="100" time="500" />
    </animation>
  </include>
  <include name="Animation_Spinning_CD_Info">
    <animation type="WindowOpen" reversible="false" condition="!Window.Previous(Startup) + Skin.HasSetting(Spinning_CDART)">
      <effect type="slide" start="0" end="180" center="960,900" time="2900" delay="1700" tween="cubic" easing="out" />
      <effect type="fade" start="0" end="100" time="900" />
    </animation>
  </include>
  <include name="Animation_Spinning_CD_Info2">
    <effect type="fade" start="-4000" end="100" time="900" />
    <animation type="WindowOpen" reversible="true" condition="!Window.Previous(Startup) + Skin.HasSetting(Spinning_CDART)">
      <effect type="slide" time="500" start="0,495" end="0,0" delay="0" />
      <effect type="slide" start="0" end="150" center="960,900" time="1300" delay="1500" easing="out" />
    </animation>
  </include>
  <include name="Animation_Spinning_CD">
    <animation type="WindowOpen" reversible="false" condition="Window.Previous(Startup)">
      <effect type="zoom" start="180" end="100" center="640,600" time="2500" tween="cubic" easing="out" />
      <effect type="fade" start="0" end="100" time="2000" />
    </animation>
    <animation type="WindowOpen" reversible="false" condition="!Window.Previous(Startup)">
      <effect type="slide" start="-230" end="-22" center="960,900" time="2900" delay="1700" tween="cubic" easing="out" />
      <effect type="fade" start="0" end="100" time="900" />
    </animation>
    <animation type="WindowClose" condition="!Player.Playing">
      <effect type="slide" start="0" end="-171" time="0" tween="cubic" easing="in" delay="0" />
      <effect type="fade" start="100" end="0" time="600" />
    </animation>
    <animation effect="rotate" end="-360" center="auto" time="6000" loop="true" reversible="false" condition="!Player.Paused">Conditional</animation>
  </include>
  <include name="Animation_ButtonFocusFade">
    <animation effect="fade" start="0" end="100" time="200">Focus</animation>
    <animation effect="fade" start="100" end="0" time="200">Unfocus</animation>
  </include>
  <include name="Animation_Spinning_CD_Triangle">
    <animation type="WindowOpen" reversible="false" condition="!Window.Previous(Startup) + Skin.HasSetting(Spinning_CDART)">
      <effect type="slide" start="0" end="-75" center="960,900" time="2900" delay="1300" tween="cubic" easing="out" />
      <effect type="fade" start="0" end="100" time="900" />
    </animation>
  </include>
  <include name="VisibleFadeEffect">
    <animation effect="fade" time="200">Visible</animation>
    <animation effect="fade" time="200">Hidden</animation>
  </include>
  <include name="Animation_CommonFade">
    <animation effect="fade" time="200">Visible</animation>
    <animation effect="fade" time="200">Hidden</animation>
    <animation effect="fade" time="500">WindowOpen</animation>
    <animation effect="fade" time="200">WindowClose</animation>
  </include>
  <include name="dialogeffect1">
    <animation effect="fade" time="500">WindowOpen</animation>
    <animation effect="fade" time="500">WindowClose</animation>
  </include>
  <include name="dialogeffect2">
    <animation effect="fade" time="1400">WindowOpen</animation>
    <animation effect="fade" time="1400">WindowClose</animation>
  </include>
  <include name="Animation_Bottom_Left_CD_Open_Close">
    <animation effect="fade" time="200">VisibleChange</animation>
    <animation effect="slide" time="200" start="-525,0" end="0,0" delay="0">WindowOpen</animation>
    <animation effect="slide" time="200" start="0,0" end="-540,0" delay="0">WindowClose</animation>
  </include>
  <include name="Animation_Left_Slide_Slow">
    <animation effect="fade" time="900">VisibleChange</animation>
    <animation effect="slide" time="900" start="-525,0" end="0,0" delay="0">WindowOpen</animation>
    <animation effect="slide" time="400" start="0,0" end="-540,0" delay="0">WindowClose</animation>
  </include>
  <include name="Animation_Bottom_BG_Slide_Up">
    <animation effect="fade" time="200">VisibleChange</animation>
    <animation effect="slide" time="300" start="0,495" end="0,0" delay="40">WindowOpen</animation>
    <animation effect="slide" time="300" start="0,0" end="0,300" delay="40">WindowClose</animation>
  </include>
  <include name="Animation_Top_BG_Slide_Down">
    <animation effect="fade" time="200">VisibleChange</animation>
    <animation effect="slide" time="250" start="0,-75" end="0,0" delay="100">WindowOpen</animation>
    <animation effect="slide" time="250" start="0,0" end="0,-75" delay="100">WindowClose</animation>
  </include>
  <include name="Animation_Slide_Left">
    <animation effect="fade" time="200">VisibleChange</animation>
    <animation effect="slide" time="200" start="825,0" end="0,0" delay="0">WindowOpen</animation>
    <animation effect="slide" time="200" start="0,0" end="840,0" delay="0">WindowClose</animation>
  </include>
  <include name="Animation_Slide_Left_List">
    <animation effect="fade" time="200">VisibleChange</animation>
    <animation effect="slide" time="800" start="825,0" end="0,0" delay="0">WindowOpen</animation>
    <animation effect="slide" time="800" start="0,0" end="840,0" delay="0">WindowClose</animation>
  </include>
  <include name="Animation_MultiplexFocusZoom">
    <animation effect="zoom" center="56,150" start="100" end="110" time="400" tween="cubic" easing="inout">Focus</animation>
    <animation effect="zoom" center="56,80" start="110" end="100" time="400" tween="cubic" easing="inout">Unfocus</animation>
  </include>
  <include name="Animation_Multiplex_Menu_Popup">
    <animation effect="slide" start="0,885" end="0,0" time="500" delay="1500" condition="!Skin.HasSetting(multiplexinfoon)">WindowOpen</animation>
    <animation effect="slide" start="0,885" end="0,0">WindowClose</animation>
    <animation effect="slide" start="0,885" end="0,0" time="500">VisibleChange</animation>
  </include>
  <include name="Animation_BackgroundFade">
    <animation effect="zoom" start="110" end="130" center="auto" time="45000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
    <animation effect="slide" start="-15,-15" end="30,30" time="8000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
    <animation effect="rotate" center="540,960" start="-2" end="2" time="41000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
  </include>
  <include name="Animation_CommonFadeOpenClose">
    <animation effect="fade" time="0">Visible</animation>
    <animation effect="fade" time="0">Hidden</animation>
    <animation effect="fade" time="500">WindowOpen</animation>
    <animation effect="fade" time="200">WindowClose</animation>
  </include>
  <include name="Animation_KeyboardPopup">
    <animation type="WindowOpen" reversible="false" condition="Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="80" end="130" center="640,360" delay="200" easing="out" tween="back" time="300" />
      <effect type="fade" delay="200" start="0" end="100" time="300" />
    </animation>
    <animation type="WindowClose" reversible="false" condition="Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="130" end="80" center="640,360" easing="in" tween="back" time="300" />
      <effect type="fade" start="100" end="0" time="300" />
    </animation>
    <animation type="Visible" reversible="false" condition="Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="80" end="130" center="640,360" delay="200" easing="out" tween="back" time="300" />
      <effect type="fade" delay="200" start="0" end="100" time="300" />
    </animation>
    <animation type="Hidden" reversible="false" condition="Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="130" end="80" center="640,360" easing="in" tween="back" time="300" />
      <effect type="fade" start="100" end="0" time="300" />
    </animation>
    <animation type="WindowOpen" reversible="false" condition="!Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="80" end="100" center="640,360" delay="200" easing="out" tween="back" time="300" />
      <effect type="fade" delay="200" start="0" end="100" time="300" />
    </animation>
    <animation type="WindowClose" reversible="false" condition="!Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
      <effect type="fade" start="100" end="0" time="300" />
    </animation>
    <animation type="Visible" reversible="false" condition="!Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="80" end="100" center="640,360" delay="200" easing="out" tween="back" time="300" />
      <effect type="fade" delay="200" start="0" end="100" time="300" />
    </animation>
    <animation type="Hidden" reversible="false" condition="!Skin.HasSetting(keyboardzoom)">
      <effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
      <effect type="fade" start="100" end="0" time="300" />
    </animation>
  </include>
</includes>